<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/ripple_light">

    <item android:state_pressed="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_4"/>
            <stroke android:width="@dimen/size_1" android:color="@color/colorGrayE6"/>
            <solid android:color="@color/selector_white_pressed"/>
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_4"/>
            <stroke android:width="@dimen/size_1" android:color="@color/colorGrayE6"/>
            <solid android:color="@color/material_gray_100"/>
        </shape>
    </item>

</ripple>